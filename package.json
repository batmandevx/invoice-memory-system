{
  "name": "ai-agent-memory-system",
  "version": "1.0.0",
  "description": "A learning layer for invoice processing automation that stores reusable insights from past invoices",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:property": "jest --testNamePattern=\"Property\"",
    "dev": "ts-node src/index.ts",
    "demo": "node demo.js",
    "demo:enhanced": "node demo.js",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "npm run build"
  },
  "keywords": [
    "ai",
    "memory",
    "invoice",
    "processing",
    "automation",
    "learning",
    "typescript"
  ],
  "author": "AI Agent Memory System",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "fast-check": "^3.13.1",
    "sqlite3": "^5.1.6"
  }
}